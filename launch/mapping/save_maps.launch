<launch>

	<arg name="map_name"	default="map"/>
	<arg name="proj_map_topic" default="/map"/>

	<node pkg="octomap_server" type="octomap_saver" name="octomap_saver_full" args="-f $(arg map_name).bt"/>
	<node pkg="map_server" type="map_saver" name="map_saver" args="-f $(arg map_name) map:=$(arg proj_map_topic)" />

</launch>
